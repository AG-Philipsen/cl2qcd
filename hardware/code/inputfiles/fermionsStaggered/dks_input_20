startcondition=cold

fermact=rooted_stagg

use_chem_pot_im=true
chem_pot_im=0.123456789

use_eo=true
NS=4
NT=4

#This means periodic boundary conditions
ThetaT=0.
ThetaS=0.

#BICGSTAB sets the init-spinorfield to random values
#      CG sets the init-spinorfield to (1,1,1)
solver=BICGSTAB

#with read_multiple_configs one can switch between "EVEN" and "ODD" as parameter for the kernel
# true = EVEN, false = ODD
read_multiple_configs=false

#this specifies the acceptance precision
solver_prec=1e-8
#reference value
test_ref_val=132.236951286314138

#This imput file must not be modified!
