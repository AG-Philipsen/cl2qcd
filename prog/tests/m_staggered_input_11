startcondition=cold

mass=0.1

use_eo=false
NS=4
NT=4

#This means ANTIperiodic boundary conditions
ThetaT=1.
ThetaS=0.

#BICGSTAB sets the init-spinorfield to random values
#      CG sets the init-spinorfield to (1,1,1)
solver=BICGSTAB

#this specifies the acceptance precision
solver_prec=1e-8
#reference value
test_ref_val=447.54061279081281555

