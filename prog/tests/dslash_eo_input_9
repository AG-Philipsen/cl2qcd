use_eo=true
NS=4
NT=4

#BICGSTAB sets the init-spinorfield to random values
solver=BICGSTAB
startcondition=continue
sourcefile=conf.00200

#with pointsource one can switch between "EVEN" and "ODD" as parameter for the kernel
# true = EVEN, false = ODD
use_pointsource=true

#this specifies the acceptance precision
solver_prec=1e-8
#reference value
test_ref_val=248.33751716161072
