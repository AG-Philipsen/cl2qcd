sourcefile=conf.00200

kappa=0.15

use_eo=true
NS=4
NT=4

#with BICGSTAB, this set the init-spinorfield to random values
solver=BICGSTAB

ThetaT=1.

#with pointsource one can switch between "EVEN" and "ODD" as parameter for the kernel
# true = EVEN, false = ODD
use_pointsource=true

#this specifies the acceptance precision
solver_prec=1e-8
#reference value
test_ref_val=2106.4541568250579
