#input file 1 for f_gauge_test

# set startcondition to cold or hot to start with different gaugefield
startcondition=continue
sourcefile=conf.00200

use_eo=false
NS=4
NT=4
fermact=TWISTEDMASS
#  set this to Cg to init the spinorfield to random values
solver=BICGSTAB

#this specifies the acceptance precision
solver_prec=1e-8
