sourcefile=conf.00200

theta_fermion_temporal=1.

kappa=0.15

use_eo=true
NS=4
NT=4

#with BICGSTAB, this set the init-spinorfield to random values
solver=BICGSTAB

#with read_multiple_configs one can switch between "EVEN" and "ODD" as parameter for the kernel
# true = EVEN, false = ODD
read_multiple_configs=false

#this specifies the acceptance precision
solver_prec=1e-8
#reference value
test_ref_val=5205.5536673142005
