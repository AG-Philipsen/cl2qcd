startcondition=continue
sourcefile=conf.00200

use_eo=true
NS=4
NT=4

#This means ANTIperiodic boundary conditions
ThetaT=1.
ThetaS=0.

#with BICGSTAB, this set the init-spinorfield to random values
solver=BICGSTAB

#with read_multiple_configs one can switch between "EVEN" and "ODD" as parameter for the kernel
# true = EVEN, false = ODD
read_multiple_configs=true

#this specifies the acceptance precision
solver_prec=1e-8
#reference value
test_ref_val=1856.991830029393

#This input file must not be modified!
